{
  "name": "Image Downloader",
  "description": "Useful tool that allows users to easily download images from websites with just a few clicks",
  "version": "1.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {}
}
